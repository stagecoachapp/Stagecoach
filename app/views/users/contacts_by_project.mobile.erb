<ul>
	<% for project in @projects %>
		<% unless project.users.count == 1 %>
			<li>
				<%= link_to project.name, project %>
				<ul>
					<% for contact in project.users %>
						<% unless contact == @current_user %>
							<%= link_to contact.name, contact %>
						<% end %>
					<% end %>
				</ul>
			</li>
		<% end %>
  	<% end %>
</ul>